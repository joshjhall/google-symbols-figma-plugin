{
  "name": "google-material-symbols-figma-plugin",
  "version": "1.2.2",
  "description": "Figma plugin for generating and maintaining Google Material Symbols library",
  "keywords": [
    "figma",
    "plugin",
    "material-design",
    "material-symbols",
    "icons",
    "design-system",
    "google"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/joshjhall/google-symbols-figma-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/joshjhall/google-symbols-figma-plugin/issues"
  },
  "homepage": "https://github.com/joshjhall/google-symbols-figma-plugin#readme",
  "license": "MIT",
  "author": "Joshua Hall <josh@yaplabs.com>",
  "type": "module",
  "main": "dist/code.js",
  "ui": "dist/ui.html",
  "scripts": {
    "build": "pnpm run clean && pnpm run build:code && pnpm run build:ui",
    "build:code": "node scripts/build-code.js",
    "build:dev": "node scripts/build-code.js --dev",
    "build:ui": "pnpm run build:ui:js && pnpm run build:ui:html",
    "build:ui:html": "node scripts/build-html.js",
    "build:ui:js": "esbuild src/ui.tsx --bundle --outfile=dist/ui.js --target=chrome58 --minify",
    "clean": "rimraf dist",
    "dev": "concurrently \"pnpm run dev:code\" \"pnpm run dev:ui\"",
    "dev:code": "node scripts/build-code.js --watch",
    "dev:ui": "esbuild src/ui.tsx --bundle --outfile=dist/ui.js --target=chrome58 --watch",
    "diagnose:tokens": "tsx scripts/diagnose-tokens.ts",
    "format": "prettier --write '**/*.{ts,tsx,json,md,yml,yaml}'",
    "format:check": "prettier --check '**/*.{ts,tsx,json,md,yml,yaml}'",
    "icons:compare": "tsx scripts/compare-and-generate.ts",
    "icons:update": "tsx scripts/update-icon-list.ts --ref=master",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "prepare": "husky",
    "release": "./scripts/release.sh",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "typecheck": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "opentype.js": "^1.3.4",
    "puppeteer": "^24.22.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.38.0",
    "@figma/plugin-typings": "^1.98.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^7.0.0",
    "@types/node": "^20.19.10",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^8.2.2",
    "dotenv": "^17.2.3",
    "esbuild": "^0.23.0",
    "eslint": "^9.33.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.5",
    "semantic-release": "^24.2.7",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.19.0+sha512.c9fc7236e92adf5c8af42fd5bf1612df99c2ceb62f27047032f4720b33f8eacdde311865e91c411f2774f618d82f320808ecb51718bfa82c060c4ba7c76a32b8",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
